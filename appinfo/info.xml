<?xml version="1.0"?>
<info>
  <id>journeys</id>
  <name>Journeys</name>
  <description>Clusters your images into journeys (vacations/trips) and creates albums for each journey. Integrates with the Memories app to access image metadata.</description>
  <licence>agpl</licence>
  <author>thrillfall</author>
  <version>0.1.0</version>
  <namespace>Journeys</namespace>
  <category>multimedia</category>
  <dependencies>
    <nextcloud min-version="30" max-version="32"/>
  </dependencies>
  <commands>
    <command>OCA\Journeys\Command\FetchUserImagesCommand</command>
    <command>OCA\Journeys\Command\ListClustersCommand</command>
    <command>OCA\Journeys\Command\ClusterAndCreateAlbumsCommand</command>
  </commands>
</info>
